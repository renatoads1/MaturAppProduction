        <StackLayout Padding="0" Margin="0">
            <Frame Padding="0" Margin="0">
                <WebView 
                    
                    Source="https://www.matur.com.br/"
                    HorizontalOptions="FillAndExpand" 
                    HeightRequest="900">
                    
                </WebView>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>